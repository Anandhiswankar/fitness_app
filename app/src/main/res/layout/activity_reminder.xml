                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameLayout26"
                    android:layout_width="329dp"
                    android:layout_height="70dp"
                    app:layout_constraintBottom_toTopOf="@+id/frameLayout28"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayout27" >
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"/>
                <TextView
                    android:gravity="center"
                    android:layout_width="match_parent"
                    android:text="hi"
                    android:layout_height="match_parent"/>
                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameLayout28"
                    android:layout_width="335dp"
                    android:layout_height="70dp"
                    app:layout_constraintBottom_toTopOf="@+id/frameLayout29"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayout26">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:text="hi"
                        android:layout_height="match_parent"/>

                </FrameLayout>

                <FrameLayout
                    android:id="@+id/frameLayout29"
                    android:layout_width="330dp"
                    android:layout_height="70dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginBottom="50dp"
                    app:layout_constraintTop_toBottomOf="@+id/frameLayout28">

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"/>
                    <TextView
                        android:gravity="center"
                        android:layout_width="match_parent"
                        android:text="hi"
                        android:layout_height="match_parent"/>

                </FrameLayout>
            </android.support.constraint.ConstraintLayout>
        </LinearLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/frameLayout23"
        android:layout_width="match_parent"
        android:layout_height="72dp"
        app:layout_constraintBottom_toBottomOf="parent">

    </FrameLayout>

</android.support.constraint.ConstraintLayout>