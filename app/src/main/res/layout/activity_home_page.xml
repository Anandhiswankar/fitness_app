<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/back1"
    android:layout_height="match_parent"
    tools:context=".Home_page">

    <ImageView
        android:id="@+id/ivWork"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:layout_marginTop="56dp"
        android:scaleType="fitXY"
        android:src="@drawable/frame1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="17dp"
        android:text="My Workout"
        android:textColor="#ffff"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivWork"
        app:layout_constraintEnd_toEndOf="@+id/ivWork"
        app:layout_constraintHorizontal_bias="0.956"
        app:layout_constraintStart_toStartOf="@+id/ivWork"
        app:layout_constraintTop_toTopOf="@+id/ivWork"
        app:layout_constraintVertical_bias="0.1" />

    <ImageView
        android:id="@+id/ivexercise"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:scaleType="fitXY"
        android:src="@drawable/frame1"
        app:layout_constraintBottom_toTopOf="@+id/ivRemind"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivWork" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="17dp"
        android:text="Yoga"
        android:textColor="#ffff"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivWork"
        app:layout_constraintEnd_toEndOf="@+id/ivexercise"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/ivexercise"
        app:layout_constraintTop_toTopOf="@+id/ivexercise"
        app:layout_constraintVertical_bias="0.013" />

    <ImageView
        android:id="@+id/ivRemind"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:scaleType="fitXY"
        android:src="@drawable/frame1"
        app:layout_constraintBottom_toTopOf="@+id/ivDiet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivexercise" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="18dp"
        android:text="reminders"
        android:textColor="#ffff"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivRemind"
        app:layout_constraintEnd_toEndOf="@+id/ivRemind"
        app:layout_constraintStart_toStartOf="@+id/ivRemind"
        app:layout_constraintTop_toTopOf="@+id/ivRemind" />

    <ImageView
        android:id="@+id/ivDiet"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:scaleType="fitXY"
        android:src="@drawable/frame1"
        app:layout_constraintBottom_toTopOf="@+id/ivAbout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivRemind" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="18dp"
        android:text="Diet Plans"
        android:textColor="#ffff"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivDiet"
        app:layout_constraintEnd_toEndOf="@+id/ivDiet"
        app:layout_constraintStart_toStartOf="@+id/ivDiet"
        app:layout_constraintTop_toTopOf="@+id/ivDiet" />

    <ImageView
        android:id="@+id/ivAbout"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:scaleType="fitXY"
        android:src="@drawable/frame1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivDiet" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:layout_marginStart="53dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="47dp"
        android:layout_marginBottom="18dp"
        android:text="About"
        android:textColor="#ffff"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/ivAbout"
        app:layout_constraintEnd_toEndOf="@+id/ivAbout"
        app:layout_constraintStart_toStartOf="@+id/ivAbout"
        app:layout_constraintTop_toTopOf="@+id/ivAbout" />

</android.support.constraint.ConstraintLayout>